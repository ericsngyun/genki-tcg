{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.genkitcg.app",
        "EXPO_PUBLIC_WS_URL": "wss://api.genkitcg.app",
        "EXPO_PUBLIC_ENVIRONMENT": "production",
        "EXPO_PUBLIC_PROJECT_ID": "27dbcc84-6f7b-4cbf-bfff-c320c0de0938",
        "SENTRY_DISABLE_AUTO_UPLOAD": "true"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "yunseric@gmail.com",
        "ascAppId": "6756835148",
        "appleTeamId": "6N9HSP5N2A"
      },
      "android": {
        "serviceAccountKeyPath": "${GOOGLE_SERVICE_ACCOUNT_KEY_PATH}",
        "track": "production"
      }
    }
  }
}
